<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="project.ppaya.square.yhmapper.YHGroupBoardMapper">
<select id="selectGroupBoardByGroupId" parameterType="int" resultType="GroupBoard">
SELECT
	*
FROM
	TABLE_GB
WHERE
	GROUP_ID = #{group_id}
ORDER BY
	GROUP_BOARD_ID
</select>
</mapper>
